  "version": "0.7.5",
  "author": "magicdawn",
  "type": "module",
  "main": "dist/bin.js",
  "bin": {
    "weread-spy": "dist/bin.js"
  },
  "files": [
    "dist",
    "assets",
    "CHANGELOG.md"
  ],
  "bugs": {
    "url": "https://github.com/magicdawn/weread-spy/issues"
  },
