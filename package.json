    "@types/nunjucks": "^3.2.5",
    "@types/prettier": "^2.7.3",
    "@types/sharp": "^0.31.1",
    "@types/urijs": "^1.19.22",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "esbuild": "^0.19.5",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.2",
